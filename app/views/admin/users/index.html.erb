<div class="container">
 <div class="row">
  <div class="col-md-6 offset-md-3">
      <p class="pt-5"></p>
      <div class="newuser">
<h3>User一覧管理画面</h3>

<%= link_to "新規追加", new_admin_user_path, class: "btn btn-warning mx-2" %>
</div>
<hr>
 <% @users.each do |user| %>
  id:<%= link_to user.id, admin_user_path(user) %>
